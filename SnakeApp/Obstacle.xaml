<UserControl x:Class="SnakeApp.Obstacle"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SnakeApp"
             mc:Ignorable="d" 
             d:DesignHeight="51" d:DesignWidth="51">
    <Grid x:Name="MainGrid">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Rectangle Grid.Column="1" Fill="Red"/>
        <Rectangle Grid.Row="1" Fill="Red"/>
        <Rectangle Grid.Column="1" Grid.Row="2" Fill="Red"/>
        <Rectangle Grid.Row="1" Grid.Column="2" Fill="Red"/>
        <Rectangle Grid.Column="1" Grid.Row="1" Fill="Red"/>
    </Grid>
</UserControl>
